  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^6.4.6",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "express": "^4.19.2",
    "html2canvas": "^1.4.1",
    "http-proxy-middleware": "^3.0.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.24.1",
    "react-scripts": "^5.0.1",
    "recharts": "^2.12.7",
    "styled-components": "^6.1.11",
    "web-vitals": "^4.2.1"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node proxyServer.js",
    "dev": "cross-env NODE_ENV=development concurrently \"node proxyServer.js\" \"react-scripts start\"",
    "build:blue": "export GENERATE_SOURCEMAP=false && rm -rf build-blue && react-scripts build && mv build build-blue",
    "build:green": "export GENERATE_SOURCEMAP=false && rm -rf build-green && react-scripts build && mv build build-green",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.1",
    "eslint-plugin-react": "^7.37.4",
    "typescript": "^4.9.5"
  },
  "proxy": "http://localhost:8080"
}
