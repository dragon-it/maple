{
  "compilerOptions": {
    "target": "es5",
    "types": ["styled-components"],
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "src",
    "api/notice-event/detail.js",
    "api/notice-event.js",
    "api/guild-rank.js",
    "api/guild-member.js",
    "api/guild-id.js",
    "api/guild-basic.js",
    "api/id.js",
    "api/character-info.js",
    "api/index.js"
  ]
}
