{
  "compilerOptions": {
    "target": "es5",
    "types": ["styled-components"],
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react"
  },
  "include": [
    "src",
    "api/noticeEventDetail.js",
    "api/noticeEvent.js",
    "api/guildRank.js",
    "api/guildMember.js",
    "api/guildId.js",
    "api/guildBasicInfo.js",
    "api/characterOcid.js",
    "api/characterInfo.js",
    "api/index.js"
  ]
}
